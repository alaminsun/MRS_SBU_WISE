    var MarketDocPracLocDtlViewModel = { "MKT_DP_LOC_DTL_SLNO": "", "MKT_DP_LOC_MAS_SLNO": "", "DP_LOC_CODE": "", "DP_LOC_NAME": "" };
    var MarketDocPracLocMstViewModel = { "MKT_DP_LOC_MAS_SLNO": "", "MARKET_CODE": "", "MarketDocPracLocDtlList": [] };
    var saveStatus = 1;
    // Save Or Update Data
    function Save() {

        saveStatus = 1;

        // Check Maximum Length for declared Text field
        CheckRequiredTextBoxMaxLength('MARKET_CODE', 20);
        CheckRequiredTextBoxMaxLength('MARKET_NAME', 100);

        // Check Save Proper Data
        if (saveStatus == 1) {
            MarketDocPracLocMstViewModel.MARKET_CODE = $("#MARKET_CODE").val();
            MarketDocPracLocMstViewModel.MKT_DP_LOC_MAS_SLNO = $("#MKT_DP_LOC_MAS_SLNO").val();
            MarketDocPracLocMstViewModel.SUStatus = $("#SUStatus").val();
            //MarketDocPracLocMstViewModel.MKT_DP_LOC_MAS_SLNO = $("#MKT_DP_LOC_MAS_SLNO").val();
            var dataSource = $("#PageDetailGrid").data("kendoGrid").dataSource;
            var data = dataSource.data(); // Get Detail Grid Data
            MarketDocPracLocMstViewModel.MarketDocPracLocDtlList = [];
            if (dataSource.hasChanges) {
                for (var i = 0; i <= data.length - 1; i++) {
                    if (data[i].dirty) {
                        MarketDocPracLocDtlViewModel.MKT_DP_LOC_DTL_SLNO = data[i].MKT_DP_LOC_DTL_SLNO;
                        MarketDocPracLocDtlViewModel.MKT_DP_LOC_MAS_SLNO = data[i].MKT_DP_LOC_MAS_SLNO;
                        MarketDocPracLocDtlViewModel.DP_LOC_CODE = data[i].DP_LOC_CODE;


                        MarketDocPracLocMstViewModel.MarketDocPracLocDtlList.push(MarketDocPracLocDtlViewModel);
                        MarketDocPracLocDtlViewModel = { "MKT_DP_LOC_DTL_SLNO": "", "MKT_DP_LOC_MAS_SLNO": "", "DP_LOC_CODE": "", "DP_LOC_NAME": "" };
                    }
                }
            }

            if (changeStatus == 1) {
                $.ajax({
                    url: '@Url.Action("SaveAndUpdate", "MarketDocPracLoc")',
                    data: JSON.stringify(MarketDocPracLocMstViewModel),
                    type: 'POST',
                    contentType: 'application/json;',
                    dataType: 'json',
                    success: function (response) {
                        if (response.msg.Type == 2) {
                            changeStatus = 0;
                            $("#SUStatus").val(1);
                            $('#MKT_DP_LOC_MAS_SLNO').val(response.MKT_DP_LOC_MAS_SLNO);
                            $('#MessageText').html(response.msg.Msg);
                            $("#MessageText").css({ 'color': 'green', 'font-size': 'larger', 'font-weight': 'bold', 'text-align': 'left' });
                            $.ajax({
                                url: '@Url.Action("GetSaveDiviList", "MarketDocPracLoc")',
                                type: 'GET',
                                data: { MKT_DP_LOC_MAS_SLNO: $("#MKT_DP_LOC_MAS_SLNO").val() },
                                success: function (data33) {
                                    $("#PageDetailGrid").data("kendoGrid").dataSource.data(data33);
                                }
                            });
                        }
                        else if (response.msg.Type == 3) {
                            changeStatus = 0;
                            $('#MessageText').html(response.msg.Msg);
                            $("#MessageText").css({ 'color': 'green', 'font-size': 'larger', 'font-weight': 'bold', 'text-align': 'left' });
                            @*$.ajax({
                                url: '@Url.Action("GetSaveDiviList", "MarketDocPracLoc")',
                                type: 'GET',
                                data: { MKT_DP_LOC_MAS_SLNO: $("#MKT_DP_LOC_MAS_SLNO").val() },
                                success: function (data33) {
                                    $("#PageDetailGrid").data("kendoGrid").dataSource.data(data33);
                                }
                            });*@
                        }
                        else if (response.msg.Type == 1) {
                            $('#MessageText').html(response.msg.Msg);
                            $("#MessageText").css({ 'color': 'red', 'font-size': 'larger', 'font-weight': 'bold', 'text-align': 'left' });
                        }
                        else {
                            changeStatus = 0;
                            $('#MessageText').html(response.msg.Msg);
                            $("#MessageText").css({ 'color': 'red', 'font-size': 'larger', 'font-weight': 'bold', 'text-align': 'left' });
                        }
                    }
                });
            }
                // No Changes Exiting Data
            else {
                $("#MessageText").html("No Changes to Save.");
                $("#MessageText").css({ 'color': 'red', 'font-size': 'larger', 'font-weight': 'bold', 'text-align': 'left' });
                $("#MARKET_CODE").focus();
            }
        }
    };
