                { field: "DSG_STRENGTH_CODE", title: "Product Code", width: 130, hidden: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "MANUFACTURER_CODE", title: "Product Code", width: 130, hidden: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                
                { field: "PROD_ID", hidden: false, title: "Prod. Code", filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "PRODUCT_NAME", title: "Product Name", width: 100, filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "GENERIC_NAME", title: "Generic", width: 100, filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "MANUFACTURER_NAME", title: "Manufec.",width: 100, filterable: true,  headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "DOSAGE_FORM_NAME", title: "Dosage Form", filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "DSG_STRENGTH_NAME", title: "Dosage Strength",  filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "UNIT_PRICE", title: "Unit Price", filterable: false, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "TC_L1_DESC", title: "TC1 Name",  filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "TC_L2_DESC", title: "TC2 Name",  filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "TC_L3_DESC", title: "TC3 Name",  filterable: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
