                    <label>Unit Price</label><label style="color: red">*</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="UNIT_PRICE" name="UNIT_PRICE" tabindex="13" class="txtBox RequiredTextBox" value="" placeholder="0.00" style="text-align: right" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-3">
                    <label>Therapeutic Class Level-3 Name</label><label style="color: red">*</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="TC_L3_DESC" name="TC_L3_DESC" tabindex="6" class="txtBox RequiredTextBox" readonly="readonly" placeholder="Press F9" value="" />
                </div>

                <div class="col-lg-3">
                    <label>Avg. Prescribe Qty</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="AVG_PRESCRIBE_QTY" tabindex="14" name="AVG_PRESCRIBE_QTY" class="txtBox" value="" placeholder="0.00" style="text-align: right" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-3">
                    <label>Therapeutic Class Level-4 Name</label><label style="color: red">*</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="TC_L4_DESC" tabindex="7" name="TC_L4_DESC" class="txtBox RequiredTextBox" readonly="readonly" placeholder="Press F9" value="" />
                </div>

                <div class="col-lg-3">
                    <label>Avg. Bought Qty</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="AVG_BOUGHT_QTY" name="AVG_BOUGHT_QTY" tabindex="15" class="txtBox" value="" placeholder="0.00" style="text-align: right" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-3">
                    <label>Product Status</label>
                </div>
                <div class="col-lg-3">
                    <select id="PROD_STATUS" name="PROD_STATUS" class="txtBox" tabindex="8">
                        <option value="A">Active</option>
                        <option value="I">Inactive</option>
                    </select>
                </div>

                <div class="col-lg-3">
                    <label>Pack Size</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="PACK_SIZE" tabindex="16" name="PACK_SIZE" class="txtBox" value="" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-3">
                    <label>SBU Group</label><label style="color: red">*</label>
                </div>
                <div class="col-lg-3">
                    <select id="SBU_GROUP" name="SBU_GROUP" class="txtBox" tabindex="17">
                        <option value="SBU_A">SBU_A</option>
                        <option value="SBU_B">SBU_B</option>
                        <option value="SBU_C">SBU_C</option>
                        <option value="SBU_D">SBU_D</option>
                        <option value="SBU_E">SBU_E</option>
                        <option value="SBU_N">SBU_N</option>
                    </select>
                </div>
            </div>

        </div>
    </form>
</div>
