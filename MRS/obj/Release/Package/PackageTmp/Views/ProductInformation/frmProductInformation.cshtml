                    <label>Therapeutic Class Level-1 Name</label><label style="color: red">*</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="TC_L1_DESC" name="TC_L1_DESC" tabindex="4" class="txtBox RequiredTextBox" readonly="readonly" placeholder="Press F9" value="" />
                </div>

                <div class="col-lg-3">
                    <label>Manufecturer Name</label><label style="color: red">*</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="MANUFACTURER_NAME" tabindex="12" name="MANUFACTURER_NAME" class="txtBox RequiredTextBox" readonly="readonly" placeholder="Press F9" value="" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-3">
                    <label>Therapeutic Class Level-2 Name</label><label style="color: red">*</label>
                </div>
                <div class="col-lg-3">
                    <input type="text" id="TC_L2_DESC" tabindex="5" name="TC_L2_DESC" class="txtBox RequiredTextBox" readonly="readonly" placeholder="Press F9" value="" />
                </div>

