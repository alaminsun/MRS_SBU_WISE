                    <input type="text" id="DOSAGE_FORM_NAME" tabindex="10" name="DOSAGE_FORM_NAME" class="txtBox RequiredTextBox" readonly="readonly" placeholder="Press F9" value="" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-lg-3">
                    <label>Product Code</label>
                </div>
                <div class="col-lg-3">
                    <input type="hidden" id="TC_L1_CODE" class="txtBox" />
                    <input type="hidden" id="TC_L2_CODE" class="txtBox" />
                    <input type="hidden" id="TC_L3_CODE" class="txtBox" />
                    <input type="hidden" id="TC_L4_CODE" class="txtBox" />
