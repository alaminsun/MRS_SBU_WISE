<script>

    var btnDisable = '@ViewBag.USER_ID';
    @*var canDisplay = '@ViewBag.CanDisplay' | true;*@
    var colHide = false;
    //if(canDisplay )
    if (btnDisable == "MRS_9")
        
        $('#btnSubmit').attr('disabled', true);
    

    //$("#").kendoButton({
    //    enable: false
    //});
    //$("#PROD_ID").attr("disabled", "disabled");
    //$("#PageDetailGrid").kendoGrid({
    //    columns: [
    //      { field: "firstProperty" },
    //      { field: "anotherProperty", hidden: !canDisplay }    
    //    ]
    //***************************** To Declare Global Variable ****************************//
    var changeStatus = 0;
    var resetEditDeleteStatus = 0;
    var dataitem = {};


    //************************* START Document.Ready Finction ******************************//
    $(document).ready(function () {
        function checkRole()
        {
            if(btnDisable === "MRS_9")
            {
                
            }
        };
        checkRole();
        $("#PRODUCT_NAME").focus();

        NumericAndDecimalValue('UNIT_PRICE');
        NumericAndDecimalValue('AVG_PRESCRIBE_QTY');
        NumericAndDecimalValue('AVG_BOUGHT_QTY');
        //NumericAndDecimalValue(PACK_SIZE);


        //************************* Handling DELETE event for DELETING ROW from DETAIL GRID ******************************//
        function handleDelete(event) {
            if (btnDisable === "MRS_9") {
                alert("Your are not authorized to Delete");
            }
            else
            {
                dataitem = grid.dataItem($(event.currentTarget).closest("tr"));
                if (changeStatus == 0) {
                    OpenConfirmationDialog();
                }
                else {
                    resetEditDeleteStatus = 3; //Delete Button
                    OpenSaveChangesDialog();
                }
            }
        
        };


        //function Role(event){}

        //function setDisabled(disabled) {
        //    //if (enabled) {
        //    //    $(".k-add-button").removeClass("k-state-disabled").addClass("k-grid-add");
        //    //}
            
        //}

        //function disableButtons() {
        //    setTimeout(function () {
        //        $(".k-grid-pager a").addClass("k-state-disabled")
        //    })
        //}

        //************************* EDIT Button for DETAIL GRID******************************//

        function handleEdit(event) {
            if (btnDisable === "MRS_9") {
                alert("Your are not authorized to edit");
            } else {
                // For Grid Edit Button
                //resetEditDeleteStatus = 2;
                // When Changes Data
                //if (changeStatus == 1) {
                //    dataitem = grid.dataItem($(event.currentTarget).closest("tr"));
                //    OpenSaveChangesDialog();
                //}
                    // When No Changes Data
            //    else {
                    
            //        $("#SUStatus").val(1);
            //        $("#PROD_ID").attr("disabled", "disabled");
            //}
            $(".RequiredTextBox").css("border-color", "");
            dataitem = grid.dataItem($(event.currentTarget).closest("tr"));
            SetMasterData(dataitem);
            $("#SUStatus").val(1);
            changeStatus == 1;
            $("#PROD_ID").attr("disabled", "disabled");
            }
         
        };

       // or omit if the user does not have permission


        //************************* DEFINE DETAIL KENDO GRID ******************************//

        @*var canDisplay = '@ViewBag.CanDisplay' | false;*@
        //$("#grid").kendoGrid({
        //    co
        //      { field: "firstProperty" },
                 
           
        var dataSource = new kendo.data.DataSource({
            batch: true,
            schema: {
                model: {
                    id: "PROD_ID",
                    fields: {
                        PROD_ID: { editable: false },
                        PRODUCT_NAME: { editable: false },
                        GENERIC_NAME: { editable: false },
                        MANUFACTURER_NAME: { editable: false },
                        DOSAGE_FORM_NAME: { editable: false },
                        DSG_STRENGTH_NAME: { editable: true },
                        UNIT_PRICE: { editable: false },
                        TC_L1_CODE: { editable: false },
                        TC_L2_CODE: { editable: false },
                        TC_L3_CODE: { editable: false },
                        TC_L4_CODE: { editable: false },
                        TC_L1_DESC: { editable: false },
                        TC_L2_DESC: { editable: false },
                        TC_L3_DESC: { editable: false },
                        TC_L4_DESC: { editable: false },
              
                        GENERIC_CODE: { editable: false },
                        DOSAGE_FORM_CODE: { editable: false },
                        DSG_STRENGTH_CODE: { editable: false },
                        MANUFACTURER_CODE: { editable: false },
                        UNIT_PRICE: { editable: false },
                        AVG_PRESCRIBE_QTY: { editable: false },
                        AVG_BOUGHT_QTY: { editable: false },
                        PACK_SIZE: { editable: false },
                        SAP_CODE: { editable: false },
                        PROD_STATUS: { editable: false }
                    }
                }
            },
            pageSize: 50,
            transport: {
                read: {
                    url: "@Url.Action("GetProductInformationList", "ProductInformation")",
                    type: "GET"
                }
            }
        });

        //To Define Columns for Store Kendo Grid

        var grid = $('#PageDetailGrid').kendoGrid({
            dataSource: dataSource,
            pageable: true,
            editable: true,
            selectable: "row",
            navigatable: true,
            filterable: {
                extra: false,
                operators: {
                    string: {
                        startswith: "Starts with",
                        contains: "Contains"
                    }
                }
            },
            sortable: true,
            height: 500,
            columns: [
                //if(btnDisable("MRS_9")),
                { field: "TC_L1_CODE", title: "Prod. Code", width: 130, hidden: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center;" } },
                { field: "TC_L2_CODE", title: "Product Code", width: 130, hidden: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "TC_L3_CODE", title: "Product Code", width: 130, hidden: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
                { field: "TC_L4_CODE", title: "Product Code", width: 130, hidden: true, headerAttributes: { "class": "gridHeader", style: "font-weight: bold;font-size: 13px;text-align: center" } },
